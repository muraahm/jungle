<tr>
<td>
<%= sale.name %>
</td>

<td>
<%= sale.starts_on %>
</td>

<td>
<%= sale.ends_on %>
</td>

<td>
<% if sale.finished? %>
<span class="lable lable-danger">Finished</span>
<% elsif sale.upcomming? %>
<span class="lable lable-warning">Upcomming</span>
<% else %>
<span class="lable lable-success">Active</span>
<% end %>
</td>
<td>
<%= sale.percent_off %>
</td>



</tr>